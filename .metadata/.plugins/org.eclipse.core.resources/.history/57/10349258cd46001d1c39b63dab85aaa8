
package practice;



public class Scanner {

	public static void main(String[] args) {
		
		int a;
		float b;
		double c;
		
		Scanner sc = new Scanner(System.in);
		a = sc.nextInt();
		b = sc.nextFloat();
		c = sc.nextDouble();

		System.out.println("a=" + a);
		System.out.println("b=" + b);
		System.out.println("c="+c);
		
	}

}
