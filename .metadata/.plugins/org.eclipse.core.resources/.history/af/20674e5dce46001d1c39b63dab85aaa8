
package practice;

import java.util.Scanner;

public class ScannerSc {

	public static void main(String[] args) {

		int a;
		float b;
		double c;

		Scanner sc = new Scanner(System.in);
		a = sc.nextInt();
		b = sc.nextFloat();
		c = sc.nextDouble();
		sc.close();
		
		System.out.println("a=" + a);
		System.out.println("b=" + b);
		System.out.println("c=" + c);
	}

}
