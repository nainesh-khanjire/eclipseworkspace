
class HourlyEmployee extends Employee {


	int totalsal, salaryperhr, workhours;
	




	void computeGrossPay() {
		
		totalsal = workhours *salaryperhr;
	}

	void computeNetPay() {

	}

	public void setSalary(int sal) {
		this.totalsal=  15000;
	}
	
	public int getSalary() {
		return totalsal =sal;
	}

	

	void print() {
		System.out.println(totalsal);
			}

			
}

