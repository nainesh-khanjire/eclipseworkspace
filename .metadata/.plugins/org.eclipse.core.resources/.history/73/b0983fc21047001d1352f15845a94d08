package practice;

public class Employment {

	public static void main(String[] args) {

		Employment e1 = new Employment(); // e1 is reference variable, local variable
						//  if e1 = null; no javac error will occur
		
		// this is throw NPE error, null pointer exception error				
		
		e1.readData(); // javac error, not initialized
		e1.displayData(); 
		
		Employment e2 =  new Employment();
		e2.readData();
		e2.displayData();
	
		System.out.println("");

	}
}
