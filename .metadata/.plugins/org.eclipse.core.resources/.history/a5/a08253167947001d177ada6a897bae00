package practice;

public class Emplo {

	
	int id;
	int age;
	float salary;
	String dept;
	
	
	Emplo(int id, int age, float salary,  String dept);
	{
		
		
	}
	
	
	void displayData() {
		System.out.println("Employee id is: " + id);
		System.out.println("employee age is: " + age + " years");
		System.out.println("employee salary is: " + salary + " rupees");
		System.out.println("employee department is: " + dept);
	}
	
	
	
	
	public static void main(String[] args) {

		Emplo e1 = new Emplo();
		e1.displayData();
		
		
		Emplo e2 = new Emplo(123,23,30000,"prod"); // e2 object created by passing values
		
		

	}

}
