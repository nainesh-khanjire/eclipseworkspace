package practice;

public class PtoNp {

	
	public static void main(String[] args) {
		
		
		
		int a= 12;
		float b = 2.5F;
		double c = 1.23D;
		
		String s1 = String.valueOf(a);
		String s1 = String.valueOf(b);
		String s1 = String.valueOf(c);
		
		System.out.println(a);
		System.out.println(b);
		System.out.println(c);
	}
}
